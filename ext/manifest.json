{
  "manifest_version": 3,
  "name": "Audio Auto-Play Bug",
  "version": "0.0.0",
  "permissions": [],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content-script.js"],
    "match_about_blank": true,
    "all_frames": true
  }],
  "web_accessible_resources": [{
    "resources": ["example.html"],
    "matches": ["<all_urls>"]
  }]
}